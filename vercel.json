{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "env": {
    "DOCS_DOMAIN": "docs.noahdummett.com"
  },
  "regions": ["iad1"],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "docs.noahdummett.com"
        }
      ],
      "destination": "/docs/:path*",
      "permanent": false
    }
  ]
}
